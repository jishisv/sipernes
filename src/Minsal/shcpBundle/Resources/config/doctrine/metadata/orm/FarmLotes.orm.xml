<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="FarmLotes" table="farm_lotes">
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="lote" type="string" column="lote" length="60" nullable="false"/>
    <field name="preciolote" type="decimal" column="preciolote" precision="5" scale="2" nullable="false"/>
    <field name="fechavencimiento" type="date" column="fechavencimiento" nullable="false"/>
    <field name="idestablecimiento" type="integer" column="idestablecimiento" nullable="false"/>
    <field name="idmodalidad" type="integer" column="idmodalidad" nullable="false"/>
  </entity>
</doctrine-mapping>
