<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="RxArea" table="rx_area">
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="codigoArea" type="string" column="codigo_area" length="4" nullable="true"/>
    <field name="nombreArea" type="string" column="nombre_area" length="150" nullable="true"/>
    <field name="habilitado" type="string" column="habilitado" length="1" nullable="true"/>
    <field name="direccion" type="string" column="direccion" length="50" nullable="true"/>
  </entity>
</doctrine-mapping>
