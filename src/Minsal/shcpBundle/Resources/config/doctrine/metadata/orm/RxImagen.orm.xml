<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="RxImagen" table="rx_imagen">
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="idRadiografia" type="integer" column="id_radiografia" nullable="true"/>
    <field name="imagen" type="blob" column="imagen" nullable="true"/>
    <field name="nombre" type="string" column="nombre" length="50" nullable="true"/>
    <field name="tipo" type="string" column="tipo" length="50" nullable="true"/>
  </entity>
</doctrine-mapping>
